{
  "$schema": "http://json.schemastore.org/swcrc",
  "jsc": {
    "baseUrl": ".",
    "target": "esnext",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "importAssertions": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "loose": false,
    "externalHelpers": true
  },
  "module": {
    "type": "es6"
  },
  "minify": true,
  "isModule": true
}

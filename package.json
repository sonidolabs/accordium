{
  "name": "accordium",
  "version": "1.0.0",
  "author": "Gabriel Batista",
  "main": "./dist/accordium.cjs",
  "exports": {
    "import": "./dist/accordium.mjs",
    "require": "./dist/accordium.cjs"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonidolabs/accordium.git"
  },
  "description": "Light weight accordion for your project.",
  "scripts": {
    "build:common": "npx swc --config-file common.swcrc ./src/accordium.ts -o ./dist/accordium.cjs",
    "build:esm": "npx swc --config-file esm.swcrc ./src/accordium.ts -o ./dist/accordium.mjs",
    "build": "npm run build:common && npm run build:esm"
  },
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "typescript": "^5.3.3"
  },
  "esnext": {
    "browser": "chrome"
  },
  "bugs": {
    "url": "https://github.com/sonidolabs/accordium/issues"
  }
}
